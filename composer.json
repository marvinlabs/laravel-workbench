{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.3.0",
    "fideloper/proxy": "~4.1",
    "graham-campbell/markdown": "^10.0",
    "laravel/framework": ">=5.8.0",
    "marvinlabs/laravel-html-bootstrap-4": "*@dev",
    "marvinlabs/laravel-html-font-awesome": "*@dev",
    "marvinlabs/laravel-menus": "*@dev",
    "marvinlabs/laravel-luhn": "*@dev",
    "marvinlabs/laravel-discord-logger": "*@dev",
    "spatie/laravel-model-status": "*@dev",
    "spatie/laravel-permission": "*@dev",
    "ext-json": "*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "filp/whoops": "~2.3",
    "fzaninotto/faker": "~1.4",
    "laravel/homestead": "^8.0",
    "mockery/mockery": "^1.2",
    "nunomaduro/collision": "~3.0",
    "phpunit/phpunit": "~8.0",
    "orchestra/testbench": "~3.8",
    "symfony/dom-crawler": "^4.1",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "MarvinLabs\\Html\\Bootstrap\\Tests\\": "packages/marvinlabs/laravel-html-bootstrap-4/tests",
      "MarvinLabs\\Menus\\Tests\\": "packages/marvinlabs/laravel-menus/tests",
      "MarvinLabs\\Luhn\\Tests\\": "packages/marvinlabs/laravel-luhn/tests",
      "MarvinLabs\\DiscordLogger\\Tests\\": "packages/marvinlabs/laravel-discord-logger/tests",
      "Spatie\\ModelStatus\\Tests\\": "packages/spatie/laravel-model-status/tests",
      "Spatie\\Permission\\Test\\": "packages/spatie/laravel-permission/tests"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "path",
      "url": "packages/marvinlabs/laravel-html-font-awesome",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "packages/marvinlabs/laravel-discord-logger",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "packages/marvinlabs/laravel-html-bootstrap-4",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "packages/marvinlabs/laravel-menus",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "packages/marvinlabs/laravel-luhn",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "packages/spatie/laravel-model-status",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "packages/spatie/laravel-permission",
      "options": {
        "symlink": true
      }
    }
  ]
}
